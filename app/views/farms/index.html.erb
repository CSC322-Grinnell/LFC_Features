
<h>
  Farm Information
</h>
<%= form_for :farm do |f| %>
  <p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
  </p>

  <p>
  <%= f.label :address %><br>
  <%= f.text_area :address %>
  </p>
  
  <p>
  <%= f.label :url %><br>
  <%= f.text_field :url %>
  </p> 
  
  <p>
  <%= f.label :phone %><br>
  <%= f.text_field :phone %>
  </p>
  
  <p>
  <%= f.label :facebook %><br>
  <%= f.text_field :facebook %>
  </p>
  
  <p>
  <%= f.label :instagram %><br>
  <%= f.text_field :instagram %>
  </p>
  
  <p>
  <%= f.label :twitter %><br>
  <%= f.text_field :twitter %>
  </p>
  
  <p>
  <%= f.submit "Submit" %>
  </p>
  <% end %>



